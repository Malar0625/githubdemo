package oops;

public class college_mark 
{
	int rno;
	String name;
	int java, maths;
	float avg;
	public void average()
	{
		avg=(java+maths)/2;
		
	}
	public void display()
	{
		System.out.println("Roll number : " +rno);
		System.out.println("Name : " +name);
		System.out.println("Maths mark : " +maths);
		System.out.println("Java mark : " +java);
		System.out.println("Average : " +avg);
	}

}
