package polimorphism;

public class method_overriding 
{
	class base_overriding
	{

	public int total()
	{
		//return(x+y);
		 System.out.println("one");
		return 0; 
	}
	}
 static class derived_overriding extends base_overriding
{
	public int total()
	{
		//return(x*y);
		 System.out.println("two");
		return 0;
	}


	 
	public static void main(String[] args)
	{
		derived_overriding obj = new derived_overriding(); 
		
		obj. total();

	}	

}
}
