package exercise_1;

public class String_example_four 
{

	public static void main(String[] args) 
	{
		String s="i got job in cts";
		for (int i=0;i<=s.length()-1;i++)
		{
			if(s.substring(i).indexOf(' ',0) i==0)
			{
			for(int j=0;j<=s.length()-1;j++)
			{
				System.out.println(i);
			}
		}

	}
}	
}
