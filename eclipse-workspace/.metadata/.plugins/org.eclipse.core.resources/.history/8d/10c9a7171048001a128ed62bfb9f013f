package exercise_1;

public class String_example_three
{

	public static void main(String[] args)
	{
		String s="i got job in cts";
		int count = 1;
		int l=s.length();
		for(int i=0;i<l-1;i++)
		{
			int r=s.charAt(i);
			if(r==' ')
			{
				count++;
				
			}
			
		}
		System.out.println(count);
	}

}
