package collections;

import collections.ArrayList_demo;

public class ArrayList_demo 
{
	int rno;
	String name;
	int java, maths;
	float avg;
	public void average()
	{
		avg=(java+maths)/2.0f;
		System.out.println(avg);
	}

	public static void main(String[] args)
	{
		ArrayList_demo stu=new ArrayList_demo();
		stu.java=80;
		stu.maths=85;
		stu.average();

	}

}
