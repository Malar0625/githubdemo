package exercise_1;

public class String_example_five
{

	public static void main(String[] args) 
	{
		int p1=0, p2=0;
		String s="i got job in cts";
		while(p1!=-1)
		{
			p2=s.indexOf(' ',0);
			if(p2==-1)
			{
				p2=s.length();
				String s1=s.substring(p1,p2);
				break;
			}
			else
			{
				String s2=s.substring(p1,p2);
				p1=p2+1;
				System.out.println(s2);
				
			}
		}
	}

}
