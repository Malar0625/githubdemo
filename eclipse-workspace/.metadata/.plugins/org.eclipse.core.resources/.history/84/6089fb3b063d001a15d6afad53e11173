package exercise_1;

public class abstract_class 
{

	public static void main(String[] args)
	{
		abstract class Bike
		{  
			abstract void run();  
		}  
		class Honda extends Bike
		{  
		void run()
		{
			System.out.println("abc");
		}  
		public void main(String args[])
		{  
		Bike obj = new Honda();  
		 obj.run();  
		}  
	}  

}

}
