package exercise_1;

public class pair_number_add 
{

	public static void main(String[] args) 
	{
		int[] n= {21,14,7,15,-2,15,22,9,6};
		int sum = 0, a=36;
		for(int i=0;i<=8;i++)
		{
			for(int j=1;j<=7;j++)
			{
				if(n[i]+n[j]==a)
				{
					System.out.println(n[i]+ "+" + n[j]+ "=" +a);
					
				}
			}
		}
		

	}

}
