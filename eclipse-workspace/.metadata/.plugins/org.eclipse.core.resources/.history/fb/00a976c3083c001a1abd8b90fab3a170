package exercise_1;

public class odd_numbers
{
	public static void main(String[] args) 
	{
		int i, sum=0;
		System.out.println("The Odd Numbers are:");
		for (i = 1; i <= 5; i++) 
		{
			if (i % 2 != 0) 
			{
				sum=sum+i;
			System.out.print(i + " ");
			
			
			}

		}

		
	}
}
